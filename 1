zad 1
import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {
        //1
        Scanner scanner = new Scanner(System.in);
        System.out.println("Podaj pierwsza liczbe:");
        double liczba1 = scanner.nextDouble();
        System.out.println("Podaj druga liczbe:");
        double liczba2 = scanner.nextDouble();
        System.out.println( "Wynik: "+(liczba1+liczba2));
        System.out.println( "Wynik: "+(liczba1-liczba2));
        System.out.println( "Wynik: "+(liczba1*liczba2));
        System.out.println( "Wynik: "+(liczba1/liczba2));
        //2
        System.out.println("Wprowadz pierwszy ciag: ");
        String str1 = scanner.next();
        System.out.println("Wprowadz drugi ciag: ");
        String str2 = scanner.next();

        System.out.println("Ciag 1:" + str1);
        System.out.println("Ciag 2: " + str2);
        //3
        System.out.println("Podaj pierwsza liczbe:");
        double liczba11 = scanner.nextDouble();
        System.out.println("Podaj druga liczbe:");
        double liczba12 = scanner.nextDouble();
        System.out.println("Podaj trzecia liczbe:");
        double liczba13 = scanner.nextDouble();
        System.out.println("Podaj czwarta liczbe:");
        double liczba14 = scanner.nextDouble();

        double suma = liczba11+liczba12+liczba13+liczba14;
        System.out.println("Suma:" + suma);

        scanner.close();
        //4
        System.out.println("Podaj bok:");
        double kwadrat = scanner.nextDouble();
        
        double pole = kwadrat * kwadrat;
        System.out.println("Pole:" + pole);
    }
}
zad2
import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {

        Scanner scanner = new Scanner(System.in);

        System.out.println("Podaj rok:");
        int rok =  scanner.nextInt();

        boolean przestepny = false;

        if((rok%4==0 && rok%100!=0) || (rok % 400==0))
        {
            przestepny = true;
        }

        if(przestepny)
        {
            System.out.println("tak");

        }
        else
        {
            System.out.println("nie");
        }
    }
}
zad 3(odwrocona)
import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {

        Scanner scanner = new Scanner(System.in);

        System.out.println("Podaj liczbe: ");
        int liczba = scanner.nextInt();

        int odwrocona = odwroc(liczba);
        System.out.println("liczba po: " + odwrocona);


    }

    public static int odwroc(int liczba){
        int n = 0;
        while(liczba !=0){
            int cyfra = liczba % 10;
            n = n * 10 + cyfra;
            liczba /= 10;
        }
        return n;
    }
}
